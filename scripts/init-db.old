        for table in tables:
            result = db.session.execute(text(f"SELECT MAX(id) FROM {table}")).scalar()
            if result is None:
                # Tabla vac칤a  pr칩ximo ID = 1
                db.session.execute(text(f"SELECT setval('{table}_id_seq', 1, false)"))
